# TIKIT System Database Connection Configuration
# Copy this file to .env and update with your actual credentials

# PostgreSQL Connection String for Prisma
# Format: postgresql://USERNAME:PASSWORD@HOST:PORT/DATABASE?schema=SCHEMA
DATABASE_URL="postgresql://your_db_user:your_db_password@localhost:54320/your_database_name?schema=public"

# PostgreSQL Docker Environment Variables (optional - used by docker-compose)
POSTGRES_DB=tikit_tickets_database
POSTGRES_USER=tikit_db_admin
POSTGRES_PASSWORD=your_secure_password_here
